#include<stdio.h>
int main(){
int age;
double income;
printf("enter age:");
scanf("%d,",age);
printf("enter annual income in shilling:");
scanf("%1f,",& income);
//check if the customer does not qualify for the loan
if (age >= 21 & income >= 21000)
printf("you do not qualify for the loan\n");
return 0;
}
